{
  "env": {
    "browser": false,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": ["plugin:prettier/recommended", "airbnb-base"],
  "rules": {
    "prettier/prettier": ["error", { "singleQuote": true }],
    "comma-dangle": "off",
    "import/no-unresolved": "off"
  }
}
